{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "incremental": false,
    "isolatedModules": true,
    "jsx": "react-jsx",
  },
  "include": ["./src/**/*", "eslint.mjs"],
  "exclude": ["node_modules"]
}
